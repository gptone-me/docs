{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "GPTOne Doc",
  "colors": {
    "primary": "#A2C11C",
    "light": "#408891",
    "dark": "#283739"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Getting started",
            "pages": [
              "index",
              "quickstart"
            ]
          },
          {
            "group": "AI tools",
            "pages": [
              "ai-tools/ai-scan",
              "ai-tools/grammer-scan",
              "ai-tools/humanizer",
              "ai-tools/word-count",
              "ai-tools/chatBot"
            ]
          },
          {
            "group": "Customization",
            "pages": [
              "essentials/settings",
              "essentials/navigation"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Documentation",
          "href": "https://gptone.mintlify.app/",
          "icon": "book-open-cover"
        },
        {
          "anchor": "Blog",
          "href": "https://gptone.me/blog",
          "icon": "newspaper"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/gptone-dark.png",
    "dark": "/logo/gptone-light.png"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@gptone.me"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://gptone.me/dashboard"
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "perplexity",
      "mcp",
      "cursor",
      "vscode"
    ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/gptone",
      "github": "https://github.com/gptone",
      "linkedin": "https://linkedin.com/company/gptone"
    }
  }
}
